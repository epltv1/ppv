{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/live/:path*",
      "destination": "/live/:path*.html"
    },
    {
      "source": "/category/:id",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/:path*.html"
    }
  ]
}
